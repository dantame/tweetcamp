<div class="qa-tweet">
  <div class="qa-tweet-sidebar">
    <span class="qa-tweet-avatar">
      <img src="<%= tweet.user.profile_image_url %>"/>
    </span>
  </div>
  <div class="qa-tweet-header">
    <span class="qa-tweet-name"><%= tweet.user.name %></span>
    <span class="qa-tweet-screen-name">@<%= tweet.user.screen_name %></span>
    <span class="qa-tweet-date"><%= tweet.created_at %></span>
  </div>
  <div class="qa-tweet-body">
    <span class="qa-tweet-message"><%= tweet.text %></span>
  </div>
  <div class="qa-tweet-footer">
    <span class="qa-tweet-reply"></span>
    <span class="qa-tweet-retweet"></span>
    <span class="qa-tweet-favorite"></span>
  </div>
</div>
