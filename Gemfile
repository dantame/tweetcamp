# A sample Gemfile
source 'https://rubygems.org'

gem 'sinatra'
gem 'sinatra-contrib'
gem 'twitter'
gem 'oauth'
gem 'dotenv'

group :test, :development do
  gem 'rake'
  gem 'capybara'
  gem 'site_prism'
  gem 'rubocop', require: false
  gem 'guard'
  gem 'guard-rubocop'
  gem 'guard-rspec', require: false
end
